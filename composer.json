{
    "name": "webusup/elevenlabs-for-wp",
    "description": "A WordPress plugin that creates audio readings for posts",
    "type": "wordpress-plugin",
    "require": {
        "webusup/elevenlabs-php-sdk": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WebUsUp\\ElevenLabsForWp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Justin Maurer",
            "email": "justin@justinmaurer.dev"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
